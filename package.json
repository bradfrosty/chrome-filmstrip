{
  "name": "chrome-filmstrip",
  "version": "0.1.0",
  "description": "Generate a combined filmstrip video from multiple Chrome performance profiles",
  "type": "module",
  "bin": {
    "chrome-filmstrip": "node --experimental-wasm-threads ./dist/cli.js"
  },
  "module": "./dist/index.js",
  "exports": "./dist/index.js",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "start": "node --experimental-wasm-threads dist/cli.js"
  },
  "keywords": ["chrome", "performance", "filmstrip", "lab", "communication"],
  "author": "Brad Frost <code@bradfrost.me>",
  "license": "MIT",
  "engines": {
    "node": "^16"
  },
  "devDependencies": {
    "@types/node": "^16",
    "tsup": "^6.2.2",
    "type-fest": "^2.18.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@ffmpeg/core": "^0.11.0",
    "@ffmpeg/ffmpeg": "^0.11.0",
    "yargs": "^17.5.1"
  }
}
